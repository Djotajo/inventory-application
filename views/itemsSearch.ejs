<div class="items">
  <% items.forEach(item => { %>
  <div class="item">
    <a href="items/<%= encodeURIComponent(item.model) %>"
      ><p><%= item.name %> <%= item.model %></p>
      <img src="<%= item.image %>" alt="" class="watch-img"
    /></a>
    <form method="POST" action="/delete/<%= item.id %>">
      <input type="hidden" name="id" value="<%= item.id %>" />
      <button type="submit">Delete</button>
    </form>
  </div>

  <% }); %>
</div>
<a href="<%= back %>" class="back-button">Back</a>
